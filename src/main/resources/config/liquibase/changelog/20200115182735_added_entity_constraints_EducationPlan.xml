<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity EducationPlan.
    -->
    <changeSet id="20200115182735-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="education_plans_id"
                                 baseTableName="education_plan"
                                 constraintName="fk_education_plan_education_plans_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entrustment_plan"/>

        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="education_plan"
                                 constraintName="fk_education_plan_courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

    </changeSet>
</databaseChangeLog>
